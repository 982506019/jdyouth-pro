/* exam.wxss */
.card{
  background: #ffffff;
  margin: 20px auto;
  width: 90%;
  min-height: 200rpx;
  border-radius: 10px; 
  color: #999;
  box-sizing: border-box;
  padding: 20px 0;
}

.cell{
  width: 100%;
  height: 35px;
  line-height: 35px;
  box-sizing: border-box;
  padding: 0 15px;
  display: flex;
  justify-content: flex-start;
}
.title{
  width: 20%;
  color: #666;
}
.desc{
  width: 80%;
  text-align: right;
}